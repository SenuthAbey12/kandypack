<svg width="120" height="80" viewBox="0 0 120 80" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="logoGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#667eea;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#764ba2;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f093fb;stop-opacity:1" />
    </linearGradient>
    <filter id="logoShadow" x="-20%" y="-20%" width="140%" height="140%">
      <feDropShadow dx="2" dy="2" stdDeviation="3" flood-opacity="0.3"/>
    </filter>
  </defs>
  
  <!-- Main package shape -->
  <g transform="translate(20, 15)">
    <!-- Package base -->
    <rect x="0" y="20" width="50" height="35" fill="url(#logoGradient)" rx="6" filter="url(#logoShadow)"/>
    
    <!-- Package top -->
    <polygon points="0,20 10,10 60,10 50,20" fill="rgba(255,255,255,0.3)"/>
    
    <!-- Package side -->
    <polygon points="50,20 60,10 60,45 50,55" fill="rgba(0,0,0,0.15)"/>
    
    <!-- Tape -->
    <rect x="20" y="15" width="10" height="45" fill="#FFD700" opacity="0.8" rx="2"/>
    
    <!-- K letter highlight -->
    <path d="M 25,25 L 30,25 L 35,35 L 30,45 L 25,45 M 30,35 L 35,25" 
          stroke="white" stroke-width="2" fill="none" stroke-linecap="round"/>
  </g>
  
  <!-- Text "KP" -->
  <text x="75" y="35" font-family="Arial, sans-serif" font-size="20" font-weight="bold" fill="url(#logoGradient)">K</text>
  <text x="88" y="35" font-family="Arial, sans-serif" font-size="20" font-weight="bold" fill="url(#logoGradient)">P</text>
  
  <!-- Decorative elements -->
  <circle cx="95" cy="15" r="3" fill="rgba(102, 126, 234, 0.4)"/>
  <circle cx="105" cy="25" r="2" fill="rgba(240, 147, 251, 0.5)"/>
  <circle cx="10" cy="60" r="2.5" fill="rgba(79, 172, 254, 0.4)"/>
</svg>